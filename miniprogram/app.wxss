.cos-navgt {
  background-color: #fff;
}

.cos-navgt .navgt-l {
  width: 100rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
}

.cos-navgt .navgt-r {
  position: relative;
  padding-right: 100rpx;
  width: calc(100% - 100rpx);
  height: 100rpx;
  line-height: 100rpx;
}

.cos-navgt .navgt-r-input {
  width: 100%;
  height: 100%;
  font-size: 32rpx;
}

.cos-navgt .iconicon_signin_line {
  font-size: 36rpx;
  color: #666;
}

.cos-navgt .icon-arrows {
  position: absolute;
  right: 0;
  bottom: 0;
  width: 100rpx;
  text-align: center;
  color: #999;
  font-size: 36rpx;
}

.cos-navgt.active .iconicon_signin_line {
  color: #38acff;
}

.cos-navgt.active .navgt-r-input {
  color: #38acff;
}
