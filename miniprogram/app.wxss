view, navigator, text {
  box-sizing: border-box;
}

.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
}

page {
  background-color: #eee;
}

.clearfix {
  zoom: 1;
}

.clearfix:after {
  content: '';
  display: block;
  clear: both;
}

.fl {
  float: left;
}

.fr {
  float: right;
}

.border-bottom-out {
  position: relative;
}

.border-bottom-out:after {
  position: absolute;
  width: 100%;
  height: 1px;
  left: 0;
  bottom: 0;
  background: #e5e5e5;
  content: "";
  transform: scaleY(-.5);
  transform-origin: 50% 100%;
}

.border-top-out {
  position: relative;
}

.border-top-out:after {
  position: absolute;
  width: 100%;
  height: 1px;
  left: 0;
  top: 0;
  background: #e5e5e5;
  content: "";
  transform: scaleY(-.5);
  transform-origin: 50% 0;
}

.border-left-out {
  position: relative;
}

.border-left-out:after {
  position: absolute;
  width: 1px;
  height: 100%;
  left: 0;
  top: 0;
  background: #e5e5e5;
  content: "";
  transform: scaleX(-.5);
  transform-origin: 0 50%;
}

.border-right-out {
  position: relative;
}

.border-right-out:after {
  position: absolute;
  width: 1px;
  height: 100%;
  right: 0;
  top: 0;
  background: #e5e5e5;
  content: "";
  transform: scaleX(-.5);
  transform-origin: 100% 50%;
}

